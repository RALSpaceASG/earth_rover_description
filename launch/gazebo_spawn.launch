<launch>
	<include file="$(find agribot)/launch/description.launch" />
	<include file="$(find agribot)/launch/control.launch" />

	<!-- Run a python script to the send a service call to gazebo_ros to spawn a URDF robot -->
	<node name="spawn_agribot_model" pkg="gazebo_ros" type="spawn_model" args="-urdf -model agribot -param robot_description -z 5"/>
</launch>
